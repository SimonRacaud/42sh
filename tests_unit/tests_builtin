[001]
NAME="WHERE-01"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  where
[001-END]

[002]
NAME="WHERE"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  where wc
[002-END]

[003]
NAME="WHERE"
